{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Yarp": "Debug",
      "HarborGate": "Debug"
    }
  },
  "HarborGate": {
    "DockerSocket": "/var/run/docker.sock",
    "HttpPort": 8080,
    "HttpsPort": 8443,
    "EnableHttps": true,
    "LogLevel": "Debug",
    "Ssl": {
      "CertificateProvider": "SelfSigned",
      "CertificateStoragePath": "./certs",
      "LetsEncrypt": {
        "Email": "admin@example.com",
        "UseStaging": true,
        "AcceptTermsOfService": false
      }
    },
    "Oidc": {
      "RequireHttpsMetadata": false 
    }
  }
}
